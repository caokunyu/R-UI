{
  "name": "r-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {},
  "scripts": {
    "watch:less": "react-less-watcher -d ./src",
    "start": "react-scripts start&npm run watch:less",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "analyzer": "node ./scripts/build.prod.analyzer.js",
    "dev": "node ./scripts/build.dev.js",
    "stats": "BABEL_ENV='production' NODE_ENV='production' webpack --config ./node_modules/react-scripts/config/webpack.config.prod.js --profile --json > "
  },
  "devDependencies": {
    "classnames": "*",
    "github-markdown-css": "*",
    "immutable": "*",
    "less": "*",
    "marked": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-less-watcher": "*",
    "react-redux": "*",
    "react-router-dom": "*",
    "react-router-redux": "*",
    "react-scripts": "*",
    "react-transition-group": "*",
    "redux": "*",
    "redux-thunk": "*",
    "webpack-bundle-analyzer": "*"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
